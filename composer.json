{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
        "nzsregionulodzkiego/wampiriada": "dev-master"
    },

    "suggest": {
        "dragonee/silverplate-form": "Add a contact form to the site."
    },

    "repositories":[
        {
            "type": "vcs",
            "url": "ssh://git@dev.nzs.lodz.pl:7999/in/wampiriada.git"
        },
        {
            "type": "vcs",
            "url": "ssh://git@dev.nzs.lodz.pl:7999/in/core.git"
        }
    ],

    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0"
    },

    "minimum-stability": "dev",

    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },

    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    }
